<?xml version="1.0" encoding="UTF-8"?>
<config>

    <!--订阅哪些外部交易所-->
    <ApiNames names="huobi,okex,hitbtc,binance">
        <api>
            <name>huobi</name>
            <url>https://api.huobi.pro</url>
        </api>>
        <api>
            <name>okex</name>
            <url>https://www.okex.com</url>
        </api>
        <api>
            <name>hitbtc</name>
            <url>https://api.hitbtc.com</url>
        </api>
        <api>
            <name>binance</name>
            <url>https://api.binance.com</url>
        </api>
    </ApiNames>
    <!--订阅哪些合约-->
    <VecSymbols>
        <symbol>ETH-BTC</symbol>
        <symbol>EOS-BTC</symbol>
    </VecSymbols>
    <!--mongo存ticker数据-->
    <Mongo use="false">
        <mgoUrl>127.0.0.1:27017</mgoUrl>
        <dbName>wx</dbName>
        <tableName>ticker</tableName>
    </Mongo>
</config>